# 数据库配置
DB_HOST=
DB_PORT=3306
DB_USER=
DB_PASS=
DB_NAME=

# Redis配置（用于缓存）
REDIS_HOST=
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# API配置
API_PREFIX=/api/v1

# OSS配置
OSS_ENDPOINT=
OSS_ACCESS_KEY=
OSS_SECRET_KEY=
OSS_BUCKET=

# OSS自定义域名配置
OSS_CUSTOM_DOMAIN=
# 如果没有自定义域名，留空则使用默认的OSS域名
# OSS_CUSTOM_DOMAIN=

# 图片处理配置
OSS_IMAGE_STYLE_THUMBNAIL=?x-oss-process=image/resize,w_400,h_400,m_lfit,q_80
OSS_IMAGE_STYLE_MEDIUM=?x-oss-process=image/resize,w_800,h_800,m_lfit,q_85
OSS_IMAGE_STYLE_LARGE=?x-oss-process=image/resize,w_1200,h_1200,m_lfit,q_90

# OpenAI配置
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-2025-04-14
OPENAI_MAX_TOKENS=1500
OPENAI_TEMPERATURE=0.3

# 处理配置
BATCH_SIZE=5
MAX_RETRIES=3
PROCESSING_DELAY=2
MAX_CONCURRENT_REQUESTS=3

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/ai_analyzer.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5

# 安全配置
ALLOWED_HOSTS=localhost,127.0.0.1,yourposewebsite.com
CORS_ORIGINS=http://localhost:3000,https://yourposewebsite.com

# 缓存配置
CACHE_EXPIRE_TIME=3600
SEARCH_CACHE_EXPIRE_TIME=300